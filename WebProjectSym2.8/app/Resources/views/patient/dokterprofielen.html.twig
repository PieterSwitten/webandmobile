{% extends 'base.html.twig' %}
{% block title %}homepage{% endblock %}
{% block body %}
    <div class="jumbotron">
        <h1 class="text-center">Overzicht dokters</h1>
    </div>

    {% for result in results %}
        <div class="col-md-4">
            <h2>{{ result.getNaam() }} {{ result.getActhernaam() }}</h2>


            <img  height="300px" src="images/profiel/{{ result.getId() }}.jpeg" id="img/{{ result.getId() }}"/>
        </div>

    {% endfor    %}


{% endblock %}